@model IEnumerable<DistanceCalcErrorLogModel>

@{
    ViewBag.Title = "Distance Calc Error Log";
}

<h2>@ViewBag.Title (@Model.Count())</h2>

@if (Model.Count() > 0)
{
<table class="table" id="dataTable">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APIName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ErrorText)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.TrackingId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.At)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.ActivityId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityType)
        </th>

    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.APIName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ErrorText)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.TrackingId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.At)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.ActivityId)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.ActivityType)
        </td>

    </tr>
}
</table>
}