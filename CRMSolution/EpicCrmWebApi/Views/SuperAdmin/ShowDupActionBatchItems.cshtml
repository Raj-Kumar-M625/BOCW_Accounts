            @Html.DisplayNameFor(model => model.At)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AtBusiness)
        </th>
